{
  "name": "web-client-senteon",
  "version": "1.0.0",
  "private": true,
  "description": "Design, implementation and testing of an entire NinjaDoc web platform.",
  "main": "index.js",
  "author": "GlobalLogic Slovakia s.r.o. - Created by Maros Zeleny",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test-staged": "cross-env CI=true react-scripts test --bail --colors --coverage --env=jsdom",
    "eject": "react-scripts eject",
    "flow": "flow",
    "precommit": "lint-staged",
    "prepush": "lint-staged",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@humblebee/styled-components-breakpoint": "^2.1.2",
    "@storybook/addon-actions": "^3.2.6",
    "@storybook/addon-links": "^3.2.6",
    "@storybook/react": "^3.2.8",
    "axios": "^0.16.2",
    "babel-polyfill": "^6.26.0",
    "base-64": "^0.1.0",
    "chai": "^4.1.2",
    "dotenv": "^4.0.0",
    "eslint-config-react-app": "^2.0.1",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "polished": "^1.7.0",
    "prop-types": "^15.5.10",
    "query-string": "^5.0.0",
    "react": "^16.0.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-bootstrap": "^0.31.5",
    "react-bootstrap-table": "^4.3.1",
    "react-bootstrap-time-picker": "^1.0.3",
    "react-datepicker": "^1.2.1",
    "react-dom": "^16.0.0",
    "react-moment": "^0.7.0",
    "react-recaptcha": "^2.3.5",
    "react-redux": "^5.0.6",
    "react-router-dom": "~4.2.2",
    "react-router-scroll": "^0.4.2",
    "react-scripts": "1.0.13",
    "react-select": "^1.2.1",
    "react-sortable-hoc": "^0.6.8",
    "react-star-rating-component": "^1.3.0",
    "react-stars": "^2.2.5",
    "react-styled-flexboxgrid": "^2.1.0",
    "react-toastify": "^3.0.0",
    "react-transition-group": "^1.2.1",
    "react-transitions": "^1.0.0",
    "redux": "^3.7.2",
    "redux-form": "^7.0.4",
    "redux-immutable": "^4.0.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "reselect": "~3.0.1",
    "sanitize.css": "^5.0.0",
    "styled-components": "^3.1.6",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "cross-env": "^5.1.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "flow-bin": "^0.54.1",
    "flux-standard-action": "^1.2.0",
    "http-proxy-middleware": "^0.17.4",
    "husky": "^0.14.3",
    "jest": "^21.0.2",
    "jest-cli": "^21.0.2",
    "js-cookie": "^2.1.4",
    "lint-staged": "^4.1.3",
    "prettier": "^1.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-test-renderer": "^15.6.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:8080",
      "ws": true,
      "pathRewrite": {
        "^/api": "/"
      }
    }
  }
}
